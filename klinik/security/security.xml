<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="group_klinik_manager" model="res.groups">
        <field name="name">Klinik Manager</field>
        <field name="category_id" ref="base.module_category_hidden"/>
        <field name="comment">Kelola master data klinik.</field>
    </record>

    <record id="group_klinik_user" model="res.groups">
        <field name="name">Klinik User</field>
        <field name="implied_ids" eval="[(4, ref('group_klinik_manager'))]"/>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>
</odoo>
